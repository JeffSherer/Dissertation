<article xmlns:xlink="http://www.w3.org/1999/xlink" article-type="review-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Ann Gen Psychiatry</journal-id><journal-title>Annals of General Psychiatry</journal-title><issn pub-type="epub">1744-859X</issn><publisher><publisher-name>BioMed Central</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">18652673</article-id><article-id pub-id-type="pmc">2515304</article-id><article-id pub-id-type="publisher-id">1744-859X-7-9</article-id><article-id pub-id-type="doi">10.1186/1744-859X-7-9</article-id><article-categories><subj-group subj-group-type="heading"><subject>Review</subject></subj-group></article-categories><title-group><article-title>Mourning and melancholia revisited: correspondences between principles of Freudian metapsychology and empirical findings in neuropsychiatry</article-title></title-group><contrib-group><contrib id="A1" corresp="yes" contrib-type="author"><name><surname>Carhart-Harris</surname><given-names>Robin L</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>R.carhart-harris@bris.ac.uk</email></contrib><contrib id="A2" contrib-type="author"><name><surname>Mayberg</surname><given-names>Helen S</given-names></name><xref ref-type="aff" rid="I2">2</xref><email>hmayber@emory.edu</email></contrib><contrib id="A3" contrib-type="author"><name><surname>Malizia</surname><given-names>Andrea L</given-names></name><xref ref-type="aff" rid="I1">1</xref><email>Andrea.L.Malizia@bristol.ac.uk</email></contrib><contrib id="A4" contrib-type="author"><name><surname>Nutt</surname><given-names>David </given-names></name><xref ref-type="aff" rid="I1">1</xref><email>david.j.nutt@bris.ac.uk</email></contrib></contrib-group><aff id="I1"><label>1</label>Psychopharmacology Unit, University of Bristol, Bristol, UK</aff><aff id="I2"><label>2</label>Emory University School of Medicine, Atlanta, GA 30322, USA</aff><pub-date pub-type="collection"><year>2008</year></pub-date><pub-date pub-type="epub"><day>24</day><month>7</month><year>2008</year></pub-date><volume>7</volume><fpage>9</fpage><lpage>9</lpage><ext-link ext-link-type="uri" xlink:href="http://www.annals-general-psychiatry.com/content/7/1/9"/><history><date date-type="received"><day>2</day><month>2</month><year>2008</year></date><date date-type="accepted"><day>24</day><month>7</month><year>2008</year></date></history><permissions><copyright-statement>Copyright &#x000a9; 2008 Carhart-Harris et al; licensee BioMed Central Ltd.</copyright-statement><copyright-year>2008</copyright-year><copyright-holder>Carhart-Harris et al; licensee BioMed Central Ltd.</copyright-holder><license license-type="open-access" xlink:href="http://creativecommons.org/licenses/by/2.0"><p>This is an Open Access article distributed under the terms of the Creative Commons Attribution License (<ext-link ext-link-type="uri" xlink:href="http://creativecommons.org/licenses/by/2.0"/>), which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.</p><!--<rdf xmlns="http://web.resource.org/cc/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1" xmlns:dcterms="http://purl.org/dc/terms"><Work xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" rdf:about=""><license rdf:resource="http://creativecommons.org/licenses/by/2.0"/><dc:type rdf:resource="http://purl.org/dc/dcmitype/Text"/><dc:author>
